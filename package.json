  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/supertest": "^2.0.11",
    "chai": "^4.3.4",
    "jest": "^27.2.4",
    "jest-stare": "^2.2.2",
    "supertest": "^6.1.6",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "express": "^4.17.1",
    "faker": "^5.5.3",
    "random-useragent": "^0.5.0"
  }
